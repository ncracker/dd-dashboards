        "height":14
      },
      "definition":{
        "title":"Cumulative execution time",
        "title_size":"16",
        "title_align":"center",
        "type":"query_value",
        "requests":[
          {
            "q":"sum:aws.lambda.duration.sum{$aws_account,$region,$function,$resource,$executedVersion,$account-name}",
            "aggregator":"sum"
          }
        ],
        "autoscale":true,
        "precision":0
      }
    },
    {
      "id":12,
      "layout":{
        "x":34,
        "y":0,
        "width":19,
        "height":14
      },
      "definition":{
        "title":"Total Errors Combined",
        "title_size":"16",
        "title_align":"center",
        "type":"query_value",
        "requests":[
          {
            "q":"sum:aws.lambda.errors{$aws_account,$region,$function,$resource,$executedVersion,$account-name}.as_count()",
            "aggregator":"sum",
            "conditional_formats":[
              {
                "comparator":">",
                "palette":"white_on_red",
                "value":5
              },
              {
                "comparator":"<=",
                "palette":"white_on_green",
                "value":5
              }
            ]
          }
        ],
        "autoscale":true,
        "custom_unit":" ",
        "precision":0
      }
    },
