        "show_legend":false,
        "legend_size":"0",
        "type":"timeseries",
        "requests":[
          {
            "q":"avg:aws.lambda.provisioned_concurrency_utilization{$aws_account,$region,$function,$resource,$executedVersion,$account-name} by {functionname}",
            "style":{
              "palette":"dog_classic",
              "line_type":"solid",
              "line_width":"normal"
            },
            "display_type":"line"
          }
        ],
        "yaxis":{
          "scale":"linear",
          "label":"",
