    },
    {
      "id":15,
      "layout":{
        "x":68,
        "y":0,
        "width":35,
        "height":14
      },
      "definition":{
        "title":"Error % per Function invocations (top 10)",
        "title_size":"16",
        "title_align":"left",
        "time":{
          
        },
        "type":"query_table",
        "requests":[
          {
            "q":"top(avg:aws.lambda.errors{$aws_account,$region,$function,$resource,$executedVersion,$account-name} by {functionname}.as_count()/avg:aws.lambda.invocations{$aws_account,$region,$function,$resource,$executedVersion,$account-name} by {functionname}.as_count()*100,10,'sum','desc')",
            "conditional_formats":[
              {
                "comparator":">=",
                "palette":"white_on_red",
                "value":0.5
              }
            ],
            "alias":"Error %",
            "aggregator":"avg",
            "limit":10,
            "order":"desc"
          }
        ]
      }
    },
